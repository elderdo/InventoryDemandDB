DROP VIEW AMD_OWNER.X_LP_DEMAND_FORECAST_V;

/* Formatted on 2/13/2015 1:38:20 PM (QP5 v5.256.13226.35538) */
CREATE OR REPLACE FORCE VIEW AMD_OWNER.X_LP_DEMAND_FORECAST_V
(
   LOCATION,
   PART,
   DEMAND_FORECAST_TYPE,
   PERIOD,
   PERIOD_YEAR,
   QUANTITY,
   PART_TYPE,
   SOURCE,
   LAST_MODIFIED
)
AS
   SELECT spo_location location,
          parts.spo_prime_part_no part,
          external_demand_forecast demand_forecast_type,
          begin_date period,
          calendar_year_number period_year,
          dmnd.demand_forecast quantity,
          'Consumable' part_type,
          'Consumable1' source,
          dmnd.last_update_dt last_modified
     FROM amd_dmnd_frcst_consumables dmnd,
          amd_owner.amd_national_stock_items items,
          amd_spare_parts parts,
          amd_spare_networks ntwks,
          amd_current_period_v,
          amd_spo_types_v
    WHERE     dmnd.action_code <> 'D'
          AND dmnd.nsn = items.nsn
          AND items.prime_part_no = parts.part_no
          AND parts.is_spo_part = 'Y'
          AND parts.part_no = parts.spo_prime_part_no
          AND parts.is_consumable = 'Y'
          AND parts.spo_prime_part_no IS NOT NULL
          AND dmnd.sran = ntwks.loc_id
          AND dmnd.period = calendar_year_number
   UNION
   SELECT spo_location location,
          parts.spo_prime_part_no part,
          external_demand_forecast demand_forecast_type,
          amd_current_period_v.begin_date period,
          amd_current_period_v.calendar_year_number period_year,
          dmnd.demand_forecast quantity,
          'Consumable' part_type,
          'Consumable2' source,
          dmnd.last_update_dt last_modified
     FROM amd_dmnd_frcst_consumables dmnd,
          amd_owner.amd_national_stock_items items,
          amd_spare_parts parts,
          amd_spare_networks ntwks,
          amd_current_period_v,
          amd_spo_types_v
    WHERE     dmnd.action_code <> 'D'
          AND dmnd.nsn = items.nsn
          AND items.prime_part_no = parts.part_no
          AND parts.is_spo_part = 'Y'
          AND parts.part_no = parts.spo_prime_part_no
          AND parts.is_consumable = 'Y'
          AND parts.spo_prime_part_no IS NOT NULL
          AND dmnd.sran = ntwks.loc_id
          AND dmnd.period = (SELECT calendar_year_number
                               FROM spo_period_v
                              WHERE id = amd_current_period_v.id + 1)
   UNION
   SELECT amd_utils.getspolocation (frcst.loc_sid) location,
          frcst.part_no part,
          external_demand_forecast demand_forecast_type,
          begin_date period,
          calendar_year_number period_year,
          frcst.forecast_qty quantity,
          'Repairable' part_type,
          'Repairable1' source,
          frcst.last_update_dt last_modified
     FROM amd_part_loc_forecasts frcst,
          amd_spare_parts parts,
          amd_current_period_v,
          amd_spo_types_v
    WHERE     frcst.action_code <> 'D'
          AND frcst.part_no = parts.part_no
          AND parts.is_spo_part = 'Y'
          AND parts.part_no = parts.spo_prime_part_no
          AND parts.is_repairable = 'Y'
          AND parts.spo_prime_part_no IS NOT NULL
   UNION
   SELECT amd_utils.getspolocation (frcst.loc_sid) location,
          frcst.part_no part,
          external_demand_forecast demand_forecast_type,
          spo_period_v.begin_date period,
          spo_period_v.calendar_year_number current_period_year,
          frcst.forecast_qty quantity,
          'Repairable' part_type,
          'Repairable2' source,
          frcst.last_update_dt last_modified
     FROM amd_part_loc_forecasts frcst,
          amd_spare_parts parts,
          spo_period_v,
          amd_current_period_v,
          amd_spo_types_v
    WHERE     frcst.action_code <> 'D'
          AND frcst.part_no = parts.part_no
          AND parts.is_spo_part = 'Y'
          AND parts.part_no = parts.spo_prime_part_no
          AND parts.is_repairable = 'Y'
          AND parts.spo_prime_part_no IS NOT NULL
          AND spo_period_v.id = amd_current_period_v.id + 1;


