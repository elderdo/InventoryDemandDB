     FROM escmc17v2.planner@stl_escm_link p,
          escmc17v2.planner_code@stl_escm_link c,
          escmc17v2.spo_user@stl_escm_link u
    WHERE     p.planner_code_id = c.planner_code_id
          AND p.spo_user_id = u.spo_user_id;


