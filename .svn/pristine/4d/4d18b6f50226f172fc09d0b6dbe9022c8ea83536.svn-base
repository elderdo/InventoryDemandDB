PH_PRICE_TYPE,
PH_PRICE,
PH_PRICE_FISCAL_YEAR,
PH_INDENTURE,
MDL,
SERIES,
EFFECTIVITY_TYPE,
TRIM(RANGE_FROM),
TRIM(RANGE_TO),
RANGE_FLAG,
QPEI,
CUSTOMER
FROM
tmp_a2a_part_effectivity , amd_part_header_v
WHERE
part_no = ph_part_no

=+=+=+=
