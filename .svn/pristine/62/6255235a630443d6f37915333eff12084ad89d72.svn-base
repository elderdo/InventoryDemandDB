package effectivity;

public class Location
{
	private String locSid;
	private String locId;
	private String locationName;
	private int    qty;
	private String userDefined="";
	private String derived="";

	public void setLocSid(String pLocSid)
	{
		locSid = pLocSid;
	}

	public String getLocSid()
	{
		return locSid;
	}

	public void setLocId(String pLocId)
	{
		locId = pLocId;
	}

	public String getLocId()
	{
		return locId;
	}

	public void setLocationName(String pLocationName)
	{
		locationName = pLocationName;
	}

	public String getLocationName()
	{
		return locationName;
	}

	public void setQty(int pQty)
	{
		qty = pQty;
	}

	public int getQty()
	{
		return qty;
	}

	public void setUserDefined(String pValue)
	{
		userDefined = pValue;
	}

	public String getUserDefined()
	{
		return userDefined;
	}

	public boolean isUserDefined()
	{
		if (getUserDefined().equals("Y"))
			return true;
		else
			return false;
	}

	public void setDerived(String pValue)
	{
		derived = pValue;
	}

	public String getDerived()
	{
		return derived;
	}

	public boolean isDerived()
	{
		if (getDerived().equals("Y"))
			return true;
		else
			return false;
	}

}

