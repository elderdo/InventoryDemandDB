	/**
	 *  create an html link
	 *
	 *@param  link            the href part of the link
	 *@param  label           the label for the link
	 *@param  title           the ToolTip for the link
	 *@param  urlData         any GET data to be appended
	 *@param  defaultUrlData   
	 *@return                 The link value
	 */
	protected String getLink(String link, String label, String title, String urlData, String defaultUrlData) {
		String data = "";
		if (!urlData.equals("")) {
			data = (urlData.charAt(0) == '?' ? urlData : "?" + urlData);
		} else {
			data = defaultUrlData;
		}
		return "<a title=\"" + title + "\" href=\"" + link + data + "\"><font style=\"font-size: 11px;\" color=000000 face=\"verdana,arial,helvetica\"><B>" + label + "</b></font></a> : \n";
	}


