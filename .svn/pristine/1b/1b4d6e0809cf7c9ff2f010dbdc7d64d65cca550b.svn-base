	/**
	 *@return    The string that contains a the html used for the menu.
	 */
	public String getHtml() {
		html = "<table width=\"100%\" border=0 cellpadding=0 cellspacing=0 background=\"images/index_08.jpg\"><tr>\n";
		html += "<td width=" + imageWidth + "><img src=\"" + image + "\" width=" + imageWidth + " height=" + imageHeight + " alt=\"" + imageAltText + "\"></td>\n";
		html += "<td>\n";

		html += "<font style=\"font-size: 11px;\" color=000000 face=\"verdana, arial,helvetica\"><div align=left>\n";
		String urlData = "";
		if (!link1.equals("")) {
			html += getLink(link1, label1, title1, urlData1, "");
		}
		if (!link2.equals("")) {
			html += getLink(link2, label2, title2, urlData2, "?cboPlanner=" + plannerCode + "&cboNsn=" + getNsiSidAndNsn());
		}
		if (!link3.equals("")) {
			html += getLink(link3, label3, title3, urlData3, "?cboPlanner=" + plannerCode + "&cboNsn=" + getNsiSidAndNsn());
		}
		if (!link4.equals("")) {
			html += getLink(link4, label4, title4, urlData4, "");
		}
		if (!link5.equals("")) {
			html += getLink(link5, label5, title5, urlData5, "");
		}
		if (!link6.equals("")) {
			html += getLink(link6, label6, title6, urlData6, "");
		}
        if (html.substring(html.length()-3).equals(": \n")) {
            html = html.substring(0,html.length()-4) + "\n" ;
        }
		html += "</td>\n";
		html += "</tr></table>\n";
		return html;
	}


