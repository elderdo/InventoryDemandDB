/**
 *  Description of the Class
 *
 *@author     Douglas S. Elder
 *@since      06/20/02
 */
public class FleetSizeMember {
	private Aircraft aircraft;
	private String fleetSizeName;


	/**
	 *  Constructor for the FleetSizeMember object
	 */
	public FleetSizeMember() {
		aircraft = new Aircraft();
	}


	/**
	 *  Sets the aircraft attribute of the FleetSizeMember object
	 *
	 *@param  aircraft  The new aircraft value
	 */
	public void setAircraft(Aircraft aircraft) {
		this.aircraft = aircraft;
	}


	/**
	 *  Sets the fleetSizeName attribute of the FleetSizeMember object
	 *
	 *@param  fleetSizeName  The new fleetSizeName value
	 */
	public void setFleetSizeName(String fleetSizeName) {
		this.fleetSizeName = fleetSizeName;
	}


	/**
	 *  Sets the tailNo attribute of the FleetSizeMember object
	 *
	 *@param  tailNo  The new tailNo value
	 */
	public void setTailNo(String tailNo) {
		aircraft.setTailNo(tailNo);
	}


	/**
	 *  Gets the aircraft attribute of the FleetSizeMember object
	 *
	 *@return    The aircraft value
	 */
	public Aircraft getAircraft() {
		return aircraft;
	}


	/**
	 *  Gets the fleetSizeName attribute of the FleetSizeMember object
	 *
	 *@return    The fleetSizeName value
	 */
	public String getFleetSizeName() {
		return fleetSizeName;
	}


	/**
	 *  Gets the tailNo attribute of the FleetSizeMember object
	 *
	 *@return    The tailNo value
	 */
	public String getTailNo() {
		return aircraft.getTailNo();
	}

}
