# Set root logger level to INFO and its only appender to R
log4j.rootLogger=INFO, R

log4j.appender.R=org.apache.log4j.RollingFileAppender
log4j.appender.R.File=/apps/CRON/AMD/log/WinDiff.log

log4j.appender.R.MaxFileSize=1000KB
# Keep one backup file
log4j.appender.R.MaxBackupIndex=4

log4j.appender.R.layout=org.apache.log4j.PatternLayout
log4j.appender.R.layout.ConversionPattern=%d %5p [%t] (%F:%L) %c - %m%n

log4j.logger.LPOverrideConsumables=INFO
log4j.logger.LPOverrideConsumablesFactory=INFO
log4j.logger.OnHandInvs=INFO
