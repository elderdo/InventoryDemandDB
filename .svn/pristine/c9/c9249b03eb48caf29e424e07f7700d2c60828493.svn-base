package effectivity;

import java.util.*;

public class Nsn
{
	private String  nsn;
	private String  nsiSid;
	private String  configType;
	private String  effectBy;
	private String  nomen;
	private String  primePart;
	private String  fleetSize;
	private String  updateDate;
	private String  updateId;
	private Aircrafts  acHash;
	private Locations  locations;
	private String     errorMsg="";

	public Nsn()
	{
	}

	public void setNsn(String pNsn)
	{
		nsn = pNsn;
	}

	public String getNsn()
	{
		return nsn;
	}

	public void setNsiSid(String pNsiSid)
	{
		nsiSid = pNsiSid;
	}

	public String getNsiSid()
	{
		return nsiSid;
	}

	public void setPrimePart(String pPart)
	{
		primePart = pPart;
	}

	public String getPrimePart()
	{
		return primePart;
	}

	public void setNomen(String pNomen)
	{
		nomen = pNomen;
	}

	public String getNomen()
	{
		return nomen;
	}

	public void setConfigType(String pConfigType)
	{
		configType = pConfigType;
	}

	public String getConfigType()
	{
		return configType;
	}

	public void setEffectBy(String pEffectBy)
	{
		effectBy = pEffectBy;
	}

	public String getEffectBy()
	{
		return effectBy;
	}

	public void setFleetSize(String pFleetSize)
	{
		fleetSize = pFleetSize;
	}

	public String getFleetSize()
	{
		return fleetSize;
	}

	public void setUpdateDate(String pDate)
	{
		updateDate = pDate;
	}

	public String getUpdateDate()
	{
		return updateDate;
	}

	public void setUpdateId(String pId)
	{
		updateId = pId;
	}

	public String getUpdateId()
	{
		return updateId;
	}

	public void setAircraftList(Aircrafts pAircrafts)
	{
		acHash = pAircrafts;
	}
	
	public Aircrafts getAircraftList()
	{
		return acHash;
	}

	public void setLocations(Locations pLocations)
	{
		locations = pLocations;
	}
	
	public Locations getLocations()
	{
		return locations;
	}

	public void setErrorMsg(String pError)
	{
		errorMsg = "<font color=red><b>Error: " + pError + "</b></font>";
	}
	
	public String getErrorMsg()
	{
		return errorMsg;
	}
}
